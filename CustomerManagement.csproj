<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <RootNamespace>CustomerManagement</RootNamespace>
    <GenerateProgramFile>false</GenerateProgramFile>
    <AssemblyName>CustomerManagement</AssemblyName>
    <PackageId>CustomerManagement</PackageId>
    <Authors>CustomerManagement</Authors>
    <Company>CustomerManagement</Company>
    <Product>CustomerManagement</Product>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Microsoft.EntityFrameworkCore" Version="5.0.0" />
